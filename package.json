{
  "name": "@silexlabs/grapesjs-version-flow",
  "version": "1.0.4",
  "description": "GrapesJS plugin for managing version upgrades and migrations with sequential upgrade flow and modal UI",
  "keywords": [
    "grapesjs",
    "plugin",
    "version",
    "upgrade",
    "migration",
    "flow",
    "modal",
    "ui"
  ],
  "author": "Silex Labs",
  "repository": {
    "type": "git",
    "url": "https://github.com/silexlabs/grapesjs-version-flow.git"
  },
  "bugs": {
    "url": "https://github.com/silexlabs/grapesjs-version-flow/issues"
  },
  "homepage": "https://github.com/silexlabs/grapesjs-version-flow#readme",
  "main": "dist/index.js",
  "scripts": {
    "start": "grapesjs-cli serve",
    "build": "grapesjs-cli build --patch=false",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "bump": "npm version patch -m 'Bump v%s'"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.0.5",
    "canvas": "^3.1.2",
    "grapesjs": "^0.22.12",
    "grapesjs-cli": "^4.1.3",
    "jest": "^30.0.5",
    "jsdom": "^26.1.0"
  },
  "license": "AGPL-v3"
}
